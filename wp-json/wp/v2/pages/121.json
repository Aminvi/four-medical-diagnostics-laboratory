nst mapFrame = $('#collection-map');\r\n\r\n            function updateMap(location){\r\n                if(!location) return;\r\n                const address=encodeURIComponent(`${location.address||''}, ${location.city||''}, ${location.region||''}, ${location.zip_cd||''}`);\r\n                mapFrame.attr('src',`https:\/\/www.google.com\/maps?q=${address}&output=embed`);\r\n            }\r\n\r\n            function renderResults(list,label=''){\r\n                let html='';\r\n                const heading=$('#places-heading');\r\n                const regionSpan=heading.find('.active-region');\r\n                const regionName=regionSpan.find('.region-name');\r\n                const countSpan=regionSpan.find('.count');\r\n\r\n                if(label){ regionSpan.removeClass('d-none'); regionName.text(label); }\r\n                else{ regionSpan.addClass('d-none'); regionName.text(''); }\r\n                countSpan.text(list.length);\r\n\r\n                if(list.length){\r\n                    list.forEach((item,i)=>{\r\n                        html+=`\r\n                        <div class=\"accordion-item\">\r\n                            <button class=\"accordion-button ${i !== 0 ? 'collapsed' : ''}\" \r\n                                type=\"button\" data-bs-toggle=\"collapse\" \r\n                                data-bs-target=\"#collapse${i}\" \r\n                                aria-expanded=\"${i === 0 ? 'true' : 'false'}\" \r\n                                aria-controls=\"collapse${i}\">\r\n                                ${item.city}\r\n                            <\/button>\r\n\r\n                            <div id=\"collapse${i}\" class=\"accordion-collapse collapse ${i === 0 ? 'show' : ''}\" \r\n                                data-bs-parent=\"#accordionExample\">\r\n                                <div class=\"accordion-body\">\r\n                                    <h5><span>${item.coll_lab}<\/span> ${item.coll}<\/h5>\r\n                                    ${ (item.city && item.region) ? `\r\n                                        <h6>${item.city} - ${item.region}<\/h6>\r\n                                    ` : '' }\r\n\r\n                                    ${ (item.icon1.url && item.info_tit1 && item.address) ? `\r\n                                    <div class=\"contact-item\">\r\n                                        <div class=\"icon\">\r\n                                            <img decoding=\"async\" src=\"${item.icon1.url}\" alt=\"Icon\">\r\n                                        <\/div>\r\n                                        <div class=\"content\">\r\n                                            <span>${item.info_tit1}<\/span>\r\n                                            <p>${item.address}<\/p>\r\n                                        <\/div>\r\n                                    <\/div>` : '' }\r\n\r\n                                    ${ (item.info_tit2 && item.zip_cd && item.icon2.url) ? `\r\n                                    <div class=\"contact-item\">\r\n                                        <div class=\"icon\">\r\n                                            <img decoding=\"async\" src=\"${item.icon2.url}\" alt=\"Icon\">\r\n                                        <\/div>\r\n                                        <div class=\"content\">\r\n                                            <span>${item.info_tit2}<\/span>\r\n                                            <p>${item.zip_cd}<\/p>\r\n                                        <\/div>\r\n                                    <\/div>` : '' }\r\n\r\n                                    ${ (item.icon3.url && item.info_tit3 && item.phone) ? `\r\n                                    <div class=\"contact-item\">\r\n                                        <div class=\"icon\">\r\n                                            <img decoding=\"async\" src=\"${item.icon3.url}\" alt=\"Icon\">\r\n                                        <\/div>\r\n                                        <div class=\"content\">\r\n                                            <span>${item.info_tit3}<\/span>\r\n                                            <a href=\"tel:${item.phone}\">${item.phone}<\/a>\r\n                                        <\/div>\r\n                                    <\/div>` : '' }\r\n\r\n                                    ${ (item.icon4.url && item.info_tit4 && item.email) ? `\r\n                                    <div class=\"contact-item\">\r\n                                        <div class=\"icon\">\r\n                                            <img decoding=\"async\" src=\"${item.icon4.url}\" alt=\"Icon\">\r\n                                        <\/div>\r\n                                        <div class=\"content\">\r\n                                            <span>${item.info_tit4}<\/span>\r\n                                            <a href=\"mailto:${item.email}\">${item.email}<\/a>\r\n                                        <\/div>\r\n                                    <\/div>` : '' }\r\n\r\n                                    ${ (item.icon5.url && item.info_tit5 && item.hours) ? `\r\n                                    <div class=\"contact-item\">\r\n                                        <div class=\"icon\">\r\n                                            <img decoding=\"async\" src=\"${item.icon5.url}\" alt=\"Icon\">\r\n                                        <\/div>\r\n                                        <div class=\"content\">\r\n                                            <span>${item.info_tit5}<\/span>\r\n                                            <p>${(item.hours || '').replace(\/\\\\n\/g, '<br>')}<\/p>\r\n                                        <\/div>\r\n                                    <\/div>` : '' }\r\n\r\n                                    ${ (item.icon6.url && item.info_tit6 && item.services) ? `\r\n                                    <div class=\"contact-item\">\r\n                                        <div class=\"icon\">\r\n                                            <img decoding=\"async\" src=\"${item.icon6.url}\" alt=\"Icon\">\r\n                                        <\/div>\r\n                                        <div class=\"content\">\r\n                                            <span>${item.info_tit6}<\/span>\r\n                                            <p>${item.services}<\/p>\r\n                                        <\/div>\r\n                                    <\/div>` : '' }\r\n\r\n                                    ${ (item.btn_url && item.btn_text) ? `\r\n                                        <a href=\"${item.btn_url}\" class=\"default-btn\">${item.btn_text}<\/a>\r\n                                    ` : '' }\r\n                                <\/div>\r\n                            <\/div>\r\n                        <\/div>`;\r\n                    });\r\n                }else{\r\n                    html='<div class=\"accordion-item\"><div class=\"accordion-body\"><p>No collection points found.<\/p><\/div><\/div>';\r\n                }\r\n\r\n                $('#accordionExample').html(html);\r\n\r\n                \/\/ Accordion click updates map\r\n                $('#accordionExample .accordion-button').off('click').on('click',function(){\r\n                    const index=$(this).data('index');\r\n                    updateMap(list[index]);\r\n                });\r\n\r\n                \/\/ Clear filter\r\n                $('.clear-filter').off('click').on('click',function(){\r\n                    $('.region-select').val('');\r\n                    $('.address-input, .proximity-input').val('');\r\n                    $('.service-filter').prop('checked',false);\r\n                    renderResults(window.collectionData);\r\n                    updateMap(window.collectionData[0]);\r\n                });\r\n            }\r\n\r\n            function filterData(type){\r\n                let selectedServices=$('.service-filter:checked').map(function(){return $(this).val().toLowerCase();}).get();\r\n                let filtered=[]; let label='';\r\n\r\n                if(type==='region'){\r\n                    let region=$('.region-select').val().toLowerCase();\r\n                    label=$('.region-select').val();\r\n                    filtered=window.collectionData.filter(item=>\r\n                        (region===''||(item.region||'').toLowerCase().includes(region)) &&\r\n                        (selectedServices.length===0||selectedServices.some(s=>(item.services||'').toLowerCase().includes(s)))\r\n                    );\r\n                }else if(type==='address'){\r\n                    let address=$('.address-input').val().toLowerCase();\r\n                    label=address;\r\n                    filtered=window.collectionData.filter(item=>\r\n                        (address===''||(item.address||'').toLowerCase().includes(address)) &&\r\n                        (selectedServices.length===0||selectedServices.some(s=>(item.services||'').toLowerCase().includes(s)))\r\n                    );\r\n                }else if(type==='proximity'){\r\n                    let query=$('.proximity-input').val().toLowerCase();\r\n                    label=query;\r\n                    filtered=window.collectionData.filter(item=>\r\n                        (query===''||\r\n                            (item.city||'').toLowerCase().includes(query)||\r\n                            (item.region||'').toLowerCase().includes(query)||\r\n                            (item.zip_cd||'').toLowerCase().includes(query)\r\n                        ) &&\r\n                        (selectedServices.length===0||selectedServices.some(s=>(item.services||'').toLowerCase().includes(s)))\r\n                    );\r\n                }\r\n\r\n                renderResults(filtered,label);\r\n                if(filtered.length>0) updateMap(filtered[0]);\r\n            }\r\n\r\n            $('#collection-search-region').on('submit',e=>{e.preventDefault();filterData('region');});\r\n            $('#collection-search-address').on('submit',e=>{e.preventDefault();filterData('address');});\r\n            $('#collection-search-proximity').on('submit',e=>{e.preventDefault();filterData('proximity');});\r\n\r\n            renderResults(window.collectionData);\r\n            if(window.collectionData.length>0) updateMap(window.collectionData[0]);\r\n        });\r\n        <\/script>\r\n\r\n        \t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t","protected":false},"excerpt":{"rendered":"<p>Collection Point News Home Collection Point News Search by Service: Select a Service to see the respective collection points. Clinical Analysis Performs all clinical analysis. Pathological Anatomy Receives all pathology exams. Cardiology Exams Performs ECG, Holter, and MAPA exams. COVID-19 Tests Performs COVID-19 testing. MRI &amp; CT Scan Detailed body imaging diagnostic tools. Complete Blood <\/p>\n","protected":false},"author":1,"featured_media":0,"parent":0,"menu_order":0,"comment_status":"closed","ping_status":"closed","template":"elementor_header_footer","meta":{"_acf_changed":false,"footnotes":""},"class_list":["post-121","page","type-page","status-publish","hentry"],"acf":[],"_links":{"self":[{"href":"https:\/\/themes.hibootstrap.com\/limpid\/wp-json\/wp\/v2\/pages\/121","targetHints":{"allow":["GET"]}}],"collection":[{"href":"https:\/\/themes.hibootstrap.com\/limpid\/wp-json\/wp\/v2\/pages"}],"about":[{"href":"https:\/\/themes.hibootstrap.com\/limpid\/wp-json\/wp\/v2\/types\/page"}],"author":[{"embeddable":true,"href":"https:\/\/themes.hibootstrap.com\/limpid\/wp-json\/wp\/v2\/users\/1"}],"replies":[{"embeddable":true,"href":"https:\/\/themes.hibootstrap.com\/limpid\/wp-json\/wp\/v2\/comments?post=121"}],"version-history":[{"count":52,"href":"https:\/\/themes.hibootstrap.com\/limpid\/wp-json\/w